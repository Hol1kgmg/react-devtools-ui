{
  "name": "react-web-inspector-ui",
  "version": "0.1.0",
  "description": "A React component library for debugging in environments where browser DevTools are unavailable (mobile browsers, WebViews, embedded browsers, etc.). View console logs and network requests directly in your app.",
  "keywords": [
    "web-inspector",
    "react",
    "dev-tool"
  ],
  "homepage": "https://github.com/Hol1kgmg/react-web-inspector-ui#readme",
  "bugs": {
    "url": "https://github.com/Hol1kgmg/react-web-inspector-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hol1kgmg/react-web-inspector-ui.git"
  },
  "license": "MIT",
  "author": "Hol1kgmg",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:app": "tsc -b && vite build --mode app",
    "preview": "vite preview",
    "lint": "npm-run-all -p lint:*",
    "lint:code": "oxlint --type-aware --type-check --fix && eslint --fix",
    "lint:markup": "markuplint \"src/**/*.{html,jsx,tsx}\" --fix --problem-only",
    "format": "oxfmt . --write",
    "check": "npm-run-all lint format",
    "test": "npm-run-all -p test:*",
    "test:unit": "vitest --project=unit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@markuplint/jsx-parser": "^4.7.22",
    "@markuplint/react-spec": "^4.5.22",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-oxlint": "^1.39.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.5",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "lefthook": "^2.0.15",
    "markuplint": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "oxlint-tsgolint": "^0.11.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "module": "./dist/index.js"
}
