{
  "name": "react-devtools-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "npm-run-all -p lint:*",
    "lint:code": "oxlint --type-aware --type-check --fix && eslint --fix",
    "lint:markup": "markuplint \"src/**/*.{html,jsx,tsx}\" --fix --problem-only",
    "format": "oxfmt . --write",
    "check": "npm-run-all lint format"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@markuplint/jsx-parser": "^4.7.22",
    "@markuplint/react-spec": "^4.5.22",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-oxlint": "^1.39.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.5",
    "globals": "^16.5.0",
    "lefthook": "^2.0.15",
    "markuplint": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "oxlint-tsgolint": "^0.11.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
